
{
  "manifest_version": 3,
  "name": "Weather Extension",
  "version": "1.0",
  "description": "Displays current weather based on your location.",
  "permissions": ["geolocation"],
  "host_permissions": ["https://api.openweathermap.org/"],
  "action": {
    "default_popup": "popup.html"
  }
}
